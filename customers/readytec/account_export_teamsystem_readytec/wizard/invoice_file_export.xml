<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="wizard_invoice_file_export" model="ir.ui.view">
            <field name="name">Esporta verso gecom teamsystem</field>
            <field name="model">invoice.file.export</field>
            <field name="arch" type="xml">
                <form string="Esporta gecom">
                    <group col="4">
                        <group colspan="4">
                            <field name="export_file_txt" readonly="1"/>
                        </group>
                    </group>
                    <footer>
                    	<button icon="gtk-ok" name="act_getfile" string="Export" type="object" class="oe_highlight"/>
                        or
                        <button icon="gtk-cancel" class="oe_link" special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="wizard_invoicesez_file_export" model="ir.ui.view">
            <field name="name">Esporta sezionale verso gecom teamsystem</field>
            <field name="model">invoice.file.export</field>
            <field name="arch" type="xml">
                <form string="Esporta gecom">
                    <group col="4">
                        <group colspan="4">
                            <field name="export_file_txt" readonly="1"/>
                        </group>
                    </group>
                    <footer>
                    	<button icon="gtk-ok" name="act_getfile" string="Export" type="object" class="oe_highlight"/>
                        or
                        <button icon="gtk-cancel" class="oe_link" special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="wizard_invoiceanag_file_export" model="ir.ui.view">
            <field name="name">Esporta anagrafiche verso gecom teamsystem</field>
            <field name="model">invoice.file.export</field>
            <field name="arch" type="xml">
                <form string="Esporta gecom">
                    <group col="4">
                        <group colspan="4">
                            <field name="export_file_txt" readonly="1"/>
                        </group>
                    </group>
                    <footer>
                    	<button icon="gtk-ok" name="act_getfile" string="Export" type="object" class="oe_highlight"/>
                        or
                        <button icon="gtk-cancel" class="oe_link" special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="wizard_accounting_file_export" model="ir.ui.view">
            <field name="name">Esporta gecom teamsystem</field>
            <field name="model">invoice.file.export</field>
            <field name="arch" type="xml">
                <form string="Esporta gecom">
                	 <group>
                        <group>
                            <field name="date_from"/>
                        </group>
                        <group>
                            <field name="date_to"/>
                        </group>
                        <group>
                            <field name="target_move"/>
                        </group>
                    </group>
                    <group col="4">
                        <group colspan="4">
                            <field name="export_file_txt" readonly="1"/>
                        </group>
                    </group>
                    <footer>
                    	<button icon="gtk-ok" name="act_getfile" string="Export" type="object" class="oe_highlight"/>
                        or
                        <button icon="gtk-cancel" class="oe_link" special="cancel" string="Cancel"/>
                    </footer>
                  </form>
            </field>
        </record>

       <act_window name="Esporta verso gecom teamsystem"
            res_model="invoice.file.export"
            src_model="account.invoice"
            target="new"
            key2="client_action_multi"
            id="action_wizard_invoice_file_export"
            view_id="wizard_invoice_file_export"/>
       

       <act_window name="Esporta sezionale verso gecom teamsystem"
            res_model="invoice.file.export"
            src_model="account.move"
            target="new"
            key2="client_action_multi"
            id="action_wizard_invoicesez_file_export"
            view_id="wizard_invoicesez_file_export"/>
        
         <act_window name="Esporta anagrafiche verso gecom teamsystem"
            res_model="invoice.file.export"
            src_model="res.partner"
            target="new"
            key2="client_action_multi"
            id="action_wizard_invoiceanag_file_export"
            view_id="wizard_invoiceanag_file_export"/>
          
          <act_window name="gecom teamsystem"
            res_model="invoice.file.export"
            src_model="account.move"
            target="new"
            key2="client_action_multi"
            id="action_wizard_accountingdata_file_export"
            view_id="wizard_accounting_file_export"/> 	  	      

  </data>
</openerp>
