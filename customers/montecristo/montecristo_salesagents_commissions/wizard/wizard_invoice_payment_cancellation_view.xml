<?xml version="1.0" encoding="utf-8"?>

<openerp>
    <data>

        <!--         ANNULLA PAGAMENTO DAI FATTURE WIZARD            -->
        <record id="wzd_invoice_payment_cancellation_view" model="ir.ui.view">
            <field name="name">wzd.invoice_payment_cancellation.wizard</field>
            <field name="model">wzd.invoice_payment_cancellation</field>
            <field name="inherit_id" ref="salesagent_commissions.wzd_invoice_payment_cancellation_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='note_cancellation']" position="replace">
                	<field name="salesagent_id" domain="[('salesagent', '=', True)]" colspan="4"/>
                </xpath>  
            </field>
        </record>

    </data>
</openerp>
