<?xml version="1.0"?>
<openerp>
    <data>

         <!-- Accreditation Test List Temp Form View -->
         <record model="ir.ui.view" id="view_accreditation_test_list_temp_form">
                <field name="name">accreditation.test.list.temp.form</field>
                <field name="model">accreditation.test.list.temp</field>
                <field name="arch" type="xml">       
                    <form string="Accreditation Test List Temp Form">
                        <group>
                            <field name="test_id" readonly="1" />
                            <field name="ref_excel" colspan="4" readonly="1" />
                            <field name="test_list_id" colspan="4" readonly="1" />
                            <field name="var_type_id" colspan="4" />
                            <field name="add_row" />
                            <field name="var_category" />
                            <field name="category_id" colspan="4" />
                            <field name="var_material_product_matrix" />
                            <field name="material_product_matrix" colspan="4" />
                            <field name="var_property" />
                            <field name="property" colspan="4" />
                            <field name="var_measure" />
                            <field name="measure" colspan="4" />
                            <field name="var_technique" />
                            <field name="technique" colspan="4" />
                            <field name="var_method" />
                            <field name="method" colspan="4" />
                        </group>
                    </form>
                </field>
         </record>

        <!-- Accreditation Test List Temp Tree View -->
        <record model="ir.ui.view" id="view_accreditation_test_list_temp_tree">
                <field name="name">accreditation.test.list.temp.tree</field>
                <field name="model">accreditation.test.list.temp</field>
                <field name="arch" type="xml">
                    <tree string="Accreditation Test List Temp Tree">
                        <field name="test_id" />
                        <field name="ref_excel" readonly="1" />
                        <field name="material_product_matrix" />
                        <field name="property" />
                        <field name="measure" />
                        <field name="technique" />
                        <field name="method" />
                        <field name="category_id" />
                        <field name="add_row" />
                    </tree>
                </field>
        </record>

    </data>
</openerp>
