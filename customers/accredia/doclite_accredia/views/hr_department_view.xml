<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Department View -->
        <record id="view_doclite_accreditation_department_form" model="ir.ui.view">
            <field name="name">view.doclite.accreditation.department.form</field>
            <field name="model">hr.department</field>
            <field name="inherit_id" ref="hr_accredia.view_accreditation_department_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='project_sequence_id']" position="after">
                    <field name="sequence_doclite_in_id"/>
                    <field name="sequence_doclite_out_id"/>
                    <field name="sequence_protocol_rv"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
