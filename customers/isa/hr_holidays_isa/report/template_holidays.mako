<html>

<head>
  <style type="text/css">
/*<![CDATA[*/
        @page {  }
        table { border-collapse:collapse; border-spacing:0; empty-cells:show }
        td, th { vertical-align:top; font-size:12pt;}
        h1, h2, h3, h4, h5, h6 { clear:both }
        ol, ul { margin:0; padding:0;}
        li { list-style: none; margin:0; padding:0;}
        <!-- "li span.odfLiEnd" - IE 7 issue-->
        li span. { clear: both; line-height:0; width:0; height:0; margin:0; padding:0; }
        span.footnodeNumber { padding-right:1em; }
        span.annotation_style_by_filter { font-size:95%; font-family:Arial; background-color:#fff000;  margin:0; border:0; padding:0;  }
        * { margin:0;}
        .P1 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; }
        .P10 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; }
        .P11 { font-size:2pt; font-family:Liberation Serif; writing-mode:page; }
        .P2 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; font-weight:bold; }
        .P3 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; text-align:center ! important; font-weight:bold; }
        .P4 { font-size:6pt; font-family:Liberation Serif; writing-mode:page; }
        .P5 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; background-color:#e6e6e6; }
        .P6 { font-size:11pt; font-family:Liberation Serif; writing-mode:page; background-color:#e6e6e6; }
        .P7 { font-size:12pt; font-family:LMMono10; writing-mode:page; background-color:#e6e6ff; font-weight:bold; }
        .P8 { font-size:2pt; font-family:Liberation Serif; writing-mode:page; background-color:#e6e6ff; }
        .P9 { font-size:10pt; font-family:Liberation Serif; writing-mode:page; font-weight:bold; }
        .Standard { font-size:12pt; font-family:Liberation Serif; writing-mode:page; }
        .Table1 { width:25.7cm; float:none; }
        .Table1_A1 { background-color:transparent; padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-width:0,0018cm; border-top-style:solid; border-top-color:#000000; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_A10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_A2 { background-color:transparent; padding:0.097cm; border-left-width:0,0176cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-width:0,0176cm; border-top-style:solid; border-top-color:#000000; border-bottom-width:0,0176cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_B10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_B4 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_C10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_D10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_D6 { padding:0.097cm; border-left-style:none; border-right-style:none; border-top-style:none; border-bottom-width:0,0176cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_D7 { padding:0.097cm; border-width:0,0176cm; border-style:solid; border-color:#000000; }
        .Table1_E10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_F1 { background-color:transparent; padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_F10 { padding:0.097cm; border-left-width:0,0018cm; border-left-style:solid; border-left-color:#000000; border-right-width:0,0018cm; border-right-style:solid; border-right-color:#000000; border-top-style:none; border-bottom-width:0,0018cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_F2 { background-color:transparent; padding:0.097cm; border-width:0,0176cm; border-style:solid; border-color:#000000; }
        .Table1_F6 { padding:0.097cm; border-left-width:0,0176cm; border-left-style:solid; border-left-color:#000000; border-right-style:none; border-top-style:none; border-bottom-width:0,0176cm; border-bottom-style:solid; border-bottom-color:#000000; }
        .Table1_F7 { background-color:#ccccff; padding:0.097cm; border-width:0,0176cm; border-style:solid; border-color:#000000; }
        .Table1_A { width:1.455cm; }
        .Table1_B { width:8.816cm; }
        .Table1_C { width:7.671cm; }
        .Table1_D { width:1.436cm; }
        .Table1_E { width:3.27cm; }
        .Table1_F { width:3.053cm; }
        <!-- ODF styles with no properties representable as CSS -->
        .Table1.1 .Table1.10 .Table1.3 .Table1.4 .Table1.5 .Table1.6 .Table1.7 .Table1.8 .Table1.9 { }
  /*]]>*/
  </style>
</head>

<body>

    <% setLang("it_IT") %>
    <% get_wizard_params(data["form"]["month"],data["form"]["year"]) %>

  <table border="0" cellspacing="0" cellpadding="0" class="Table1">

    <tr class="Table11">
      <td colspan="5" class="Table1_A1 c26">
        <p class="P7">${_('PERMESSI E STRAORDINARI')}</p>
      </td>

      <td class="Table1_F1 c27">
        <p class="P3">&nbsp;</p>
      </td>
    </tr>

    <tr class="Table11">

      <td class="Table1_A2 c28">
        <p class="P9">${get_date()}</p>
      </td>

      %for day in get_days():
      <td class="Table1_F2 c28">
            <p class="P1">${day}</p>
      </td>
      %endfor

      <td class="Table1_F2 c27">
        <p class="P3">${_('Totale')}</p>
      </td>
    </tr>

    %for emp in get_emps():

    <tr class="Table14">
      <td colspan="4" class="Table1_B4 c28">
        <p class="P6">${emp['name']}</p>

        <p class="P8">${get_holidays_by_month(emp['id'])}</p>
      </td>
    </tr>

    <tr class="Table11">

      <td class="Table1_F2 c28">
        <p class="P9">${get_date()}</p>
      </td>

      %for day in get_days():
      <td class="Table1_F2 c28">
            <p class="P1">${day}</p>
      </td>
      %endfor

      <td class="Table1_F2 c27">
        <p class="P3">${_('Totale')}</p>
      </td>
    </tr>

    %for hol_type in get_holiday_type():
    %if get_totals_by_type(hol_type['id']):

    <tr class="Table17">

      <td class="Table1_B10 c28">
        <p class="P1">${hol_type['name']}</p>
      </td>

     %for hol in get_holidays_by_type(hol_type['id']):
      <td class="Table1_B10 c28">
            <p class="P1">${hol}</p>
      </td>
     %endfor

      <td class="Table1_F7 c27">
        <p class="P2">${get_totals_by_type(hol_type['id'])}</p>
      </td>
    </tr>

    %endif
    %endfor

    %endfor
  </table>


</body>
</html>
