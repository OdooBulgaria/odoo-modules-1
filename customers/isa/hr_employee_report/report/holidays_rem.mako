<body class="c16">

    <% setLang("it_IT") %>

  <table border="0" cellspacing="0" cellpadding="0" class="Table1">
    <colgroup>
      <col width="15" />
      <col width="359" />
      <col width="75" />
      <col width="112" />
      <col width="187" />
      <col width="36" />
      <col width="113" />
      <col width="38" />
      <col width="75" />
      <col width="113" />
    </colgroup>

    <tr class="Table11">
      <td colspan="10" class="Table1_A1 c6">
        <p class="P6">${_('PERMESSI RIMANENTI AL')} <span class=
        "T1">${get_date()}</span></p>
      </td>
    </tr>

    %for emp in get_emps():

    <tr class="Table13">
      <td colspan="6" class="Table1_A3 c6">
        <p class="P8">${emp['name']}</p>

        <p class="P7">${get_remaining_holidays(emp['id'])}</p>
      </td>

      <td class="Table1_G3 c12">
        <p class="P1">${_('Richiesti')}</p>
      </td>

      <td colspan="2" class="Table1_H3 c13">
        <p class="P1">${_('Rimanenti')}</p>
      </td>

      <td class="Table1_J3 c14">
        <p class="P1">${_('Totali')}</p>
      </td>
    </tr>

    %for hol_type in get_holiday_type():
    %if get_remaining_holidays_by_type(hol_type[1])[2]:

    <tr class="Table16">
      <td colspan="6" class="Table1_A7 c6">
        <p class="P4">${hol_type[0]}</p>
      </td>

      <td class="Table1_G9 c12">
        <p class="P5">
        ${get_remaining_holidays_by_type(hol_type[1])[0]}</p>
      </td>

      <td colspan="2" class="Table1_H9 c13">
        <p class="P5">
        ${get_remaining_holidays_by_type(hol_type[1])[1]}</p>
      </td>

      <td class="Table1_J9 c14">
        <p class="P5">
        ${get_remaining_holidays_by_type(hol_type[1])[2]}</p>
      </td>
    </tr>

    %endif
    %endfor

