<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_hr_timesheet_invoice_create_final_omniapart" model="ir.ui.view">
            <field name="name">hr.timesheet.invoice.create.final.form.omniapart</field>
            <field name="model">hr.timesheet.invoice.create.final</field>
            <field name="inherit_id" ref="hr_timesheet_invoice.view_hr_timesheet_invoice_create_final"/>            
            <field name="arch" type="xml">
            	<xpath expr='//field[@name="date"]' position="before">
					<field name="task"/>
					<field name="contract"/>
            	</xpath>
            	<xpath expr='//field[@name="product"]' position="after">
					<field name="date_invoice"/>
            	</xpath>            	            	
            </field>
        </record>    	
    	
    </data>
</openerp>
