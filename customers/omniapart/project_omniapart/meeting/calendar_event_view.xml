<?xml version="1.0"?>
<openerp>
    <data>

    <!-- Calendar - Event Form View -->
    <record model="ir.ui.view" id="view_calendar_event_omniapart_form">
        <field name="name">Calendar - Event Form Omniapart</field>
        <field name="model">calendar.event</field>
        <field name="inherit_id" ref="calendar.view_calendar_event_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='start']" position="before">
                <field name="task_id" />
            </xpath>
        </field>
    </record>

    </data>
</openerp>
