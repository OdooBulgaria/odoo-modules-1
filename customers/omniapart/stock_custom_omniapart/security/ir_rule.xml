<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data noupdate="1">

    <record model="ir.rule" id="omniapart_stock_quants_rule">
        <field name="name">Omniapart - Quants Multicompany</field>
        <field name="model_id" ref="stock.model_stock_quant"/>     
        <field name="domain_force">[('company_id','child_of',[user.company_id.id])]</field>
    </record>    

    <record model="ir.rule" id="serial_number_portal">
        <field name="name">Serial Numbers Portal</field>
        <field name="model_id" ref="stock.model_stock_production_lot"/>     
        <field name="groups" eval="[(4,ref('base.group_portal'))]"/>        	
        <field name="domain_force">[('partner_id.id','=',user.partner_id.id)]</field>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>        
    
</data>
</openerp>
