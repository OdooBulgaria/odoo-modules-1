					<t t-snippet="snippet_blog_carousel_73lines.s_blog_carousel_no_header"
						t-thumbnail="/snippet_blog_carousel_73lines/static/src/img/s_blog-carousel-without-header.png" />
			</xpath>
		</template>

	</data>
</openerp>