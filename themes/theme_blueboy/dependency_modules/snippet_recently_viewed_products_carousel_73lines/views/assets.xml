<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="s_product_recent_carousel_place_in_tab"
			inherit_id="website.snippets">
			<xpath expr="//div[@id='snippet_content']" position="inside">
					<t
						t-snippet="snippet_recently_viewed_products_carousel_73lines.s_product_recent_view_carousel_header"
						t-thumbnail="/snippet_recently_viewed_products_carousel_73lines/static/src/img/s_product-recent-carousel-with-header.png" />
					<t
						t-snippet="snippet_recently_viewed_products_carousel_73lines.s_product_recent_carousel_no_header"
						t-thumbnail="/snippet_recently_viewed_products_carousel_73lines/static/src/img/s_product-recent-carousel-without-header.png" />
						
				    <t
                        t-snippet="snippet_recently_viewed_products_carousel_73lines.s_product_recent_view_carousel_mini_header"
                        t-thumbnail="/snippet_recently_viewed_products_carousel_73lines/static/src/img/s_product-recent-carousel-with-header.png" />
			</xpath>
		</template>
	</data>
</openerp>