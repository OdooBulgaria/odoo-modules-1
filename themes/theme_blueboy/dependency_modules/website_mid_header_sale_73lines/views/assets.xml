<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
	
		<template id="assets_frontend_css_mid_header_sale" name="Assets For Mid Header Sale"
			inherit_id="website.assets_frontend">
			<xpath expr="." position="inside">
				<link
					href="/website_mid_header_sale_73lines/static/css/mid_header_sale.css"
					rel="stylesheet" type="text/css" />
			</xpath>
		</template>
		
		<template id="s_search_box_snippet" inherit_id="website.snippets">
			<xpath expr="//div[@id='snippet_content']" position="inside">
				<div class="o_panel_body">
					<t t-snippet="website_mid_header_sale_73lines.s_search_box"
						t-thumbnail="/website_mid_header_sale_73lines/static/src/img/s_search.png" />
				</div>
			</xpath>
		</template>
	</data>
</openerp>